                                    <VisualState x:Name="Unfocused"/>
                                    <VisualState x:Name="Focused">
                                        <!--<Storyboard>
                                            <ColorAnimation Duration="0" To="#FF867F8F" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="Stroke" d:IsOptimized="True"/>
